{
  "manifest_version": 2,
  "name": "pekoraHAHA",
  "description": "pekoraHAHA",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js",
      "jquery.min.js"
    ]
  },
  "commands": {
    "save": {
      "suggested_key": {
        "default": "Alt+Shift+S"
      },
      "description": "Save a link"
    },
    "random": {
      "suggested_key": {
        "default": "Alt+Shift+L"
      },
      "description": "Load a random link"
    },
    "right": {
      "suggested_key": {
        "default": "Ctrl+Right"
      },
      "description": "right"
    }
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/"
      ],
      "js": [
        "execute.js"
      ]
    }
  ]
}